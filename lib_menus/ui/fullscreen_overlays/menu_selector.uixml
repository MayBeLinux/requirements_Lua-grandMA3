            <IndicatorControl Name="DeskLightsColorTheme" Anchors="5,1" Texture="corner15" Text="Desk Lights & &#xD;Color Theme" IndicatorIcon="DialogButtonIcon" Clicked=":CommandLine" SignalValue="menu 'DeskLightsEditor'" CloseAction="Ok" IndicatorIconAlignmentV="Center"  HelpTopic="si_desklights.html"/>

            <IndicatorControl Anchors="1,3" Texture="corner5" Text="Network" IndicatorIcon="DialogButtonIcon" Clicked=":CommandLine" SignalValue="menu 'Network'" CloseAction="Ok" />
