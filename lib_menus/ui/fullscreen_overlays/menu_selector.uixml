            <IndicatorControl Anchors="2,5" Texture="corner10" Text="Quick&#xD;Save"   Clicked=":CommandLine" SignalValue="SaveShow" CloseAction="Ok"  UserRights="Presets" />
            <IndicatorControl Anchors="3,5" Texture="corner15" Text="Show&#xD;Creator" IndicatorIcon="DialogButtonIcon" Clicked=":CommandLine" SignalValue="menu 'ShowCreator'" />

            <!--<IndicatorControl Anchors="3,5" Texture="corner15" Text="Auto&#xD;Create" IndicatorIcon="DialogButtonIcon" IndicatorIconAlignmentV="Center" Clicked=":OnCallMenu" SignalValue="AutoCreate" />-->
            <IndicatorControl Anchors="5,5" Texture="corner15" Text="Preferences and&#xD;Timings" IndicatorIcon="DialogButtonIcon" Clicked=":CommandLine" SignalValue="menu 'PreferencesAndTiming'" CloseAction="Ok" />


