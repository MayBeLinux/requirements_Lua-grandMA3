                <Item SizePolicy="Stretch" Size="1" />
                <Item SizePolicy="Stretch" Size="1" />
                <Item SizePolicy="Fixed" Size="32" />
                <Item SizePolicy="Stretch" Size="1" />
                <Item SizePolicy="Fixed" Size="16" />
            </ItemCollectColumns>


            <IndicatorControl Name="Patch" Anchors="1,1" Texture="corner5" Text="Patch" IndicatorIcon="DialogButtonIcon" Clicked=":CommandLine" SignalValue="menu 'Patch'.'Edit'" Focus="InitialFocus" CloseAction="Ok" />
