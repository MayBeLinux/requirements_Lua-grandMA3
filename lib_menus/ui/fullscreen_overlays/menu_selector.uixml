            <IndicatorControl Anchors="3,3" Texture="corner10" Text="DMX&#xD;Protocols" IndicatorIcon="DialogButtonIcon" Clicked=":CommandLine" SignalValue="menu 'ArtNet'" CloseAction="Ok" />
