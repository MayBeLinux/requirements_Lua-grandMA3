<?xml version="1.0" encoding="UTF-8"?>
<GMA3 DataVersion="0.0.36.0">
    <ShadedOverlay Name="MenuCenter" W="800" H="480" AlignmentH="Center" AlignmentV="Center" WantsModal="1" HelpTopic="wvm_menus.html">
        <TitleBar Anchors="0,0" Rows="1">
            <ItemCollectColumns>
                <Item SizePolicy="Stretch" Size="1" />
