<?xml version="1.0"?>
<GMA3>
    <Overlay Name="Playground" BackColor="Global.Background">
        <UILayoutGrid>
            <ItemCollectRows>
                <Item Name="LayoutItem 1" SizePolicy="Fixed" Size="50" />
                <Item Name="LayoutItem 2" SizePolicy="Fixed" Size="47" />
                <Item Name="LayoutItem 3" />
                <Item Name="LayoutItem 4" SizePolicy="Fixed" Size="47" />
            </ItemCollectRows>
            <ItemCollectColumns>
                <Item Name="LayoutItem 1" />
            </ItemCollectColumns>

            <TitleBar Name="TitleBar" Anchors="0,0">
                <ItemCollectRows>
                    <Item Name="LayoutItem 1" />
                </ItemCollectRows>
                <ItemCollectColumns>
                    <Item Name="LayoutItem 1" />
                    <Item SizePolicy="Fixed" Size="50" />
                </ItemCollectColumns>

                <TitleButton Name="TitleButton 1" Texture="corner1" Text="Playground" Padding="5,0,0,0" Anchors="0,0" />
                <CloseButton Name="CloseButton 2" Texture="corner2" Anchors="1,0" />
            </TitleBar>

            <UIObject Name="TabControlDummyUIObject" Texture="corner0" Text="This will be a tabcontrol later" BackColor="Global.TextActive" Anchors="0,1" />
            <UILayoutGrid Name="ContentArea" Anchors="0,2">
                <ItemCollectRows>
                    <Item Name="LayoutItem 1" SizePolicy="Fixed" Size="10" />
                    <Item Name="LayoutItem 2" SizePolicy="Fixed" Size="84" />
                    <Item Name="LayoutItem 3" SizePolicy="Fixed" Size="10" />
                    <Item Name="LayoutItem 4" SizePolicy="Fixed" Size="84" />
                    <Item Name="LayoutItem 5" SizePolicy="Fixed" Size="10" />
                    <Item Name="LayoutItem 6" SizePolicy="Fixed" Size="84" />
                    <Item Name="LayoutItem 7" />
                </ItemCollectRows>
                <ItemCollectColumns>
                    <Item Name="LayoutItem 1" SizePolicy="Fixed" Size="10" />
                    <Item Name="LayoutItem 2" SizePolicy="Fixed" Size="100" />
                    <Item Name="LayoutItem 3" SizePolicy="Fixed" Size="10" />
                    <Item Name="LayoutItem 4" SizePolicy="Fixed" Size="100" />
                    <Item Name="LayoutItem 5" SizePolicy="Fixed" Size="10" />
                    <Item Name="LayoutItem 6" SizePolicy="Fixed" Size="100" />
                    <Item Name="LayoutItem 7" SizePolicy="Fixed" Size="10" />
                    <Item Name="LayoutItem 8" SizePolicy="Fixed" Size="100" />
