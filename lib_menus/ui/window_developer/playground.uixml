                <ItemCollectColumns>
                    <Item Name="LayoutItem 1" />
                    <Item Name="LayoutItem 2" />
                    <Item Name="LayoutItem 3" />
                    <Item Name="LayoutItem 4" />
                    <Item Name="LayoutItem 5" />
                </ItemCollectColumns>

