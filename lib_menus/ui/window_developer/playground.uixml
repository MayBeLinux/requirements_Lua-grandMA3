                <DialogButton Name="DialogButton 3" Texture="corner15" Text="PopupButton3" BackColor="Global.Default" Anchors="2,0" ColorIndicator="LineEdit.Background" />
                <DialogButton Name="DialogButton 4" Texture="corner15" Text="PopupButton4" BackColor="Global.Default" Anchors="3,0" ColorIndicator="LineEdit.Background" />
                <DialogButton Name="DialogButton 5" Texture="corner15" Text="PopupButton5" BackColor="Global.Default" Anchors="4,0" ColorIndicator="LineEdit.Background" />
