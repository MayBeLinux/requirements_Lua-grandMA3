                    <Item Name="LayoutItem 13" SizePolicy="Fixed" Size="10" />
                    <Item Name="LayoutItem 14" SizePolicy="Fixed" Size="100" />
                    <Item Name="LayoutItem 15" />
                </ItemCollectColumns>

                <DialogButton Name="DialogButton" Text="DeskLock" Anchors="1,1" Clicked=":CommandLine" SignalValue="menu Desklock" ColorIndicator="Global.Text" />
                <ToggleButton Name="ToggleButtonTwoState" Text="Art-Net Out" Anchors="3,1" Target="/ShowData/DMXProtocols/Art-Net" TargetProperty="Out" />
                <PropertyControl Name="PropertyControl" Text="Art-Net Out" Anchors="5,1" Target="/ShowData/DMXProtocols/Art-Net" TargetProperty="Out" />
                <ToggleButton Name="ToggleButtonMultiState" Text="AlignmentH Disp 1" Anchors="7,1,9,1" Target="/GraphicsRoot/DisplayCollect/Display 1" TargetProperty="AlignmentH" />
                <IndicatorControl Name="FunctionButton" Text="Click me" Anchors="11,1" ColorIndicator="LineEdit.Background" />
                <IndicatorControl Name="FunctionButton2" Text="No! Me!!" Anchors="13,1" ColorIndicator="LineEdit.Background" />
                <IndicatorControl Name="IndicatorControl" Text="Art-Net Out" Anchors="1,3,3,3" ColorIndicator="LineEdit.Background" Target="/ShowData/DMXProtocols/Art-Net" TargetProperty="Out" />
                <RadioButtons Name="RadioButton" Texture="corner0" Text="RadioButton" Anchors="5,3,7,5" Target="/GraphicsRoot/DisplayCollect/Display 1" TargetProperty="AlignmentH" />
                <Button Name="Button 1" Texture="corner0" Text="Button1" Anchors="9,3" Enabled="No" />
                <Button Name="Button 2" Texture="corner0" Text="Button2" Anchors="11,3" />
                <Button Name="Button 3" Texture="corner0" Text="Button3" Anchors="13,3" />
                <Button Name="Button 4" Texture="corner0" Text="Button4" Anchors="1,5" />
