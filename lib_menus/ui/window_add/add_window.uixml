                            <IndicatorButton Name="Sequence"            Anchors="0,1" Texture="corner15" Padding="10,0,10,0" Text=" Sequence&#xD;Sheet"    Icon="object_sequence"  IconAlignmentH="Left"   ColorIndicator="PoolWindow.Sequences"       Clicked=":CommandLine" SignalValue="STORE SCREENCONTENT DEFAULT 'WindowSequenceSheet' MinW=3 MinH=4 /NC"                      CloseAction="Ok"   ToolTip="Open a Sequence Sheet window."/>
                            <IndicatorButton Name="Content"             Anchors="0,2" Texture="corner15" Padding="10,0,10,0" Text="Content Sheet"      Icon="object_content"   IconAlignmentH="Left"   ColorIndicator="PoolWindow.Sequences"       Clicked=":CommandLine" SignalValue="STORE SCREENCONTENT DEFAULT 'WindowContentSheet'  MinW=3 MinH=4 /NC"                          CloseAction="Ok"   ToolTip="Open a Content Sheet window."/>
