<?xml version="1.0" encoding="UTF-8"?>
<GMA3 DataVersion="0.0.36.0">
    <ShadedOverlay Name="AddWindowDialog" W="800" H="480" AlignmentH="Center" AlignmentV="Center" WantsModal="1" HelpTopic="wvm_add_window.html" OnLoad=":AddWindowLoaded" AdjustInitialPosition="Yes" KeyDown=":ExecuteOnKeyDown">

        <TitleBar Anchors="0,0">
            <ItemCollectRows>
                <Item SizePolicy="Stretch" Size="1" />
            </ItemCollectRows>
            <ItemCollectColumns>
                <Item SizePolicy="Stretch" Size="1" />
                <Item SizePolicy="Fixed" Size="100" />
                <Item SizePolicy="Fixed" Size="100" />
                <Item SizePolicy="Fixed" Size="50" />
            </ItemCollectColumns>
            <TitleButton Texture="corner1" Anchors="0,0" Text="Add Window" />
            <Button Name="Configure" Anchors="1,0" Text="Configure" Clicked=":CommandLine" SignalValue="Menu 'DisplayConfig'" CloseAction="Ok" ToolTip="Configure the screen." Focus="Never"/>
            <Button Name="ClearDisplay" Anchors="2,0" Text="Delete&#xD;Screen" BackColor="Button.BackgroundClear" Clicked=":CommandLine" SignalValue="DELETE SCREENCONTENT DEFAULT.THRU" CloseAction="Ok" ToolTip="Delete all windows in the user-configurable screen area." Focus="Never"/>
            <CloseButton Texture="corner2" Anchors="3,0" ToolTip="Close Add Window." Focus="Never"/>
        </TitleBar>


        <DialogFrame Name="Frame">
            <ItemCollectRows>
                <Item SizePolicy="Content" />
                <Item SizePolicy="Stretch" />
            </ItemCollectRows>
            <ItemCollectColumns>
                <Item SizePolicy="Stretch" />
            </ItemCollectColumns>

            <UITab Name="AddWindowTabs" Anchors="0,0" AvoidScroll="Yes" TabChanged=":TabChanged" UseTabRegistry="Yes" Focus="WantsFocus">
                <ItemStr Title="Common"     Value="Common" ToolTip="Show all common windows."/>
                <ItemStr Title="Data&#xD;Pools" Value="Data Pools" ToolTip="Show Data Pools."/>
                <ItemStr Title="Presets"    Value="Presets" HelpTopic="presets_pools.html" ToolTip="Show Presets."/>
                <ItemStr Title="Pools"      Value="Pools" ToolTip="Show Pools."/>
                <ItemStr Title="Tools"      Value="Tools" ToolTip="Show Tools."/>
                <ItemStr Title="More"       Value="More" HelpTopic="system_information.html" ToolTip="Show More."/>
                <ItemStr Title="All"        Value="All" ToolTip="Show All."/>
                <ItemStr Title="Developer"  Value="Developer" VisibleOnlyInAlpha="Yes"/>
            </UITab>

            <DialogContainer Name="TabContainer" Anchors="0,1" ChangeActive=":TabChanged"  >

