                            <IndicatorButton Name="WindowPlayback"      Anchors="2,0" Texture="corner15" Padding="10,0,10,0" Text="Playbacks"          Icon="object_playback"  IconAlignmentH="Left"                                               Clicked=":CommandLine" SignalValue="STORE SCREENCONTENT DEFAULT 'WindowPlaybacks'         MinW=5 MinH=2 /NC"                          CloseAction="Ok"   ToolTip="Open a Playbacks window."/>
                            <IndicatorButton Name="WindowXkeys"         Anchors="2,1" Texture="corner15" Padding="10,0,10,0" Text="Xkeys"              Icon="object_xkeys"  IconAlignmentH="Left"                                                  Clicked=":CommandLine" SignalValue="STORE SCREENCONTENT DEFAULT 'WindowXkeys'             MinW=4 MinH=4 /NC"                          CloseAction="Ok"   ToolTip="Open an Xkeys window."/>

                            <!-- 4th column   -->

                            <IndicatorButton Name="WindowCmdline"       Anchors="3,0" Texture="corner15" Padding="10,0,10,0" Text="Command Line&#xD;History"     Icon="object_cmd"  IconAlignmentH="Left"                                                  Clicked=":CommandLine" SignalValue="STORE SCREENCONTENT DEFAULT 'WindowCommandLine' MinW=4 MinH=4 /NC"                        CloseAction="Ok"   ToolTip="Open a Command Line History window."/>
                            <IndicatorButton Name="DMX"                 Anchors="3,1" Texture="corner15" Padding="10,0,10,0" Text="DMX Sheet"                    Icon="object_dmx"  IconAlignmentH="Left"                                                    Clicked=":CommandLine" SignalValue="STORE SCREENCONTENT DEFAULT 'WindowDmxSheet' MinW=3 MinH=4 /NC"                         CloseAction="Ok"   ToolTip="Open a DMX Sheet window."/>

                        </UILayoutGrid>
                    </ScrollBox>
