                            <!-- second column   -->

                            <IndicatorButton Name="Window3D"            Anchors="1,0" Texture="corner15" Padding="10,0,10,0" Text="3D Viewer"          Icon="object_3d"  IconAlignmentH="Left"         ColorIndicator="PoolWindow.RenderQualities" Clicked=":CommandLine" SignalValue="STORE SCREENCONTENT DEFAULT 'Window3D' MinW=4 MinH=4 /NC"                                         CloseAction="Ok"   ToolTip="Open a 3D Viewer window."/>
                            <IndicatorButton Name="WindowLayoutView"    Anchors="1,1" Texture="corner15" Padding="10,0,10,0" Text=" Layout Viewer"     Icon="object_layout"  IconAlignmentH="Left"     ColorIndicator="PoolWindow.Layouts"         Clicked=":CommandLine" SignalValue="STORE SCREENCONTENT DEFAULT 'WindowLayoutView' MinW=4 MinH=4 /NC"                                 CloseAction="Ok"   ToolTip="Open a Layout Viewer window."/>
                            <IndicatorButton Name="WindowSelection"     Anchors="1,2" Texture="corner15" Padding="10,0,10,0" Text="Selection Grid"     Icon="object_selgrid"  IconAlignmentH="Left"                                                Clicked=":CommandLine" SignalValue="STORE SCREENCONTENT DEFAULT 'WindowSelectionView'   MinW=4 MinH=4 /NC"                            CloseAction="Ok"   ToolTip="Open a Selection Grid window."/>
                            <IndicatorButton Name="WindowSpecial"       Anchors="1,3" Texture="corner15" Padding="10,0,10,0" Text="Special Dialog"     Icon="object_colpick"  IconAlignmentH="Left"                                                Clicked=":CommandLine" SignalValue="STORE SCREENCONTENT DEFAULT 'WindowSpecialDialog' MinW=4 MinH=4 /NC"                              CloseAction="Ok"   ToolTip="Open a Special Dialog window. "/>
                            <IndicatorButton Name="WindowMAtricks"      Anchors="1,4" Texture="corner15" Padding="10,0,10,0" Text="  MAtricks Editor"  Icon="object_matricks" IconAlignmentH="Left"    ColorIndicator="PoolWindow.Matricks"        Clicked=":CommandLine" SignalValue="STORE SCREENCONTENT DEFAULT 'WindowMatricks'          MinH=4 MinW=4 /NC"                          CloseAction="Ok"   ToolTip="Open an MAtricks Editor window."/>

